@model codeTongCucThienTai.Models.TaiLieu
@using (Html.BeginForm("ThemMoiDuLieu", "Home", FormMethod.Post, new { enctype = "multipart/form-data" }))
{
    @Html.AntiForgeryToken()
    @Html.HiddenFor(x => x.isTrash)
    <div class="form tat-toan form-horizontal form-edit">
        <div class="form-body">
            <div class="row">
                <div class="col-md-12">
                    <div class="form-group row">
                        <label class="col-md-3 label-control">Chọn tệp</label>
                        <div class="col-md-9">
                            @Html.TextBox("fileAttach", "", new { @type = "file" })
                        </div>
                    </div>
                    <div class="form-group row">
                        <div class="col-md-12 text-right">
                            <button type="submit" class="btn btn-info">Lưu dữ liệu</button>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
}


